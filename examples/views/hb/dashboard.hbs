          <div class="col-lg-4 col-md-5 col-sm-8 col-10">
            {{>dashboard-practice-card}}
          </div>
          <div class="col-lg-4 col-md-5 col-sm-8 col-10">
            {{>dashboard-practice-card}}
          </div>
        </div>
        <div class="row my-5">
          <div class="col-md-8 col-12">
            {{>dashboard-badge-card}}
          </div>
          <div class="col-md-4 col-12 mt-md-0 mt-5">
            {{>dashboard-bookmark-card}}
