                <div class="card-md">Past 6 Months</div>
              </div>
              <div class="bg-grey mt-4"></div>
            </div>
            {{>dashboard-leaderboard}}
          </div>
        </div>
        <h4 class="t-align-c mb-5 mt-5">Practice Problems</h4>
        <div class="row c-card-carousel">
          <div class="col-lg-4 col-md-5 col-sm-8 col-10">
            {{>dashboard-practice-card}}
