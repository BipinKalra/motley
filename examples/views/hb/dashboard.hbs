      {{>sidenav}}
      <div class="offset-0 offset-md-1 col-11 px-lg-5 px-md-4 px-0 dashboard py-5">
        <div class="position-relative mb-5 banner">
          <div class="banner-nav tl">
            <span><a href="#" class="white extra-bold">&lang;</a></span>
          </div>
          <img src="https://www.mymaldives.com.au/wp-content/uploads/sites/11/2016/12/Beach-Banner.jpg"
            alt="BannerImage" class="w-100 h-100">
          <div class="banner-nav tr">
            <span><a href="#" class="white extra-bold">&rang;</a></span>
          </div>
        </div>
        <div class="row">
          <div class="col-md-8 col-12">
